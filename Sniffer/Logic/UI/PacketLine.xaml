             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SnifferApp.Logic.UI"
             mc:Ignorable="d" d:DesignWidth="300" Height="28.378">
    <Grid>
        <Label x:Name="PacketDirection" Content="X2Y" HorizontalAlignment="Left" VerticalAlignment="Top" Width="29"/>
        <Label x:Name="Timestamp" Content="20:00:00.000" HorizontalAlignment="Left" Margin="29,0,0,0" VerticalAlignment="Top" Width="76"/>
        <Label x:Name="Name" Content="Packet" HorizontalAlignment="Left" Margin="105,0,0,0" VerticalAlignment="Top" Width="195"/>
